<li>
	<div class="panel">
	<p><%= micropost.content%></p>
	<small>Posted by <%= micropost.user.name %> <%= time_ago_in_words micropost.created_at %> ago.</small>
	<% if current_user == micropost.user %>
		<%= link_to "delete", micropost, method: :delete,
		data: {confirm: "Delete post?"}, title: micropost.content %>
	<% end %>
	</div>
</li>